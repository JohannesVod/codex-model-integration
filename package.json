{
  "name": "codex-model-integration",
  "displayName": "Codex Model Integration",
  "description": "An integration of the codex model by openai into VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codex-model-integration.runCodex",
    "onView:vsCodex-sidebar"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vsCodex-sidebar-view",
          "title": "VSCodex",
          "icon": "media/robot.svg"
        }
      ]
    },
    "views": {
      "vsCodex-sidebar-view": [
        {
          "type": "webview",
          "id": "vsCodex-sidebar",
          "name": "VSCodex",
          "icon": "media/heart.svg",
          "contextualTitle": "VSCodex"
        }
      ]
    },
    "commands": [{
      "command": "codex-model-integration.runCodex",
      "title": "Run Codex model"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.54.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
